{
	"name": "vscode-akkasls-tools",
	"displayName": "Akka Serverless Tools for VSCode",
	"description": "Develop, deploy, and manage Akka Serverless apps",
	"version": "0.0.2",
	"publisher": "retgits",
	"repository": {
		"url": "https://github.com/retgits/vscode-akkasls-tools"
	},
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"build": "vsce package"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"ts-loader": "^8.0.3",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"main": "./dist/extension.js",
	"activationEvents": [
		"onView:as.views.projects",
		"onView:as.views.tools",
		"onView:as.views.status",
		"onView:as.views.credentials",
		"onCommand:as.commandpalette.credentials.add",
		"onCommand:as.commandpalette.credentials.delete",
		"onCommand:as.commandpalette.auth.login",
		"onCommand:as.commandpalette.auth.logout",
		"onCommand:as.commandpalette.deploy",
		"onCommand:as.commandpalette.undeploy",
		"onCommand:as.commandpalette.expose",
		"onCommand:as.commandpalette.unexpose",
		"onCommand:as.commandpalette.invites.inviteuser"
	],
	"contributes": {
		"configuration": {
			"title": "Akka Serverless",
			"properties": {
				"akkaserverless.dryrun": {
					"type": "boolean",
					"default": false,
					"description": "DryRun prints the commands in the log and does not execute them"
				},
				"akkaserverless.logOutput": {
					"type": "boolean",
					"default": false,
					"description": "Log output from the Akka Serverless CLI"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "as",
					"title": "Akka Serverless",
					"icon": "resources/akka-icon-reverse.svg"
				}
			]
		},
		"views": {
			"as": [
				{
					"id": "as.views.projects",
					"name": "Projects"
				},
				{
					"id": "as.views.credentials",
					"name": "Credentials"
				},
				{
					"id": "as.views.status",
					"name": "Status"
				},
				{
					"id": "as.views.tools",
					"name": "Tools"
				}
			]
		},
		"commands": [
			{
				"command": "as.views.projects.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "as.views.status.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "as.views.tools.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "as.views.credentials.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "as.commandpalette.deploy",
				"title": "Deploy a service to Akka Serverless",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.undeploy",
				"title": "Remove a service from Akka Serverless",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.expose",
				"title": "Expose a service for inbound traffic",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.unexpose",
				"title": "Removes a service route from a service",
				"category": "Akka Serverless"
			},
			{
				"command": "as.views.projects.credentials.add",
				"title": "Add credentials (1)"
			},
			{
				"command": "as.views.projects.credentials.delete",
				"title": "Delete credentials"
			},
			{
				"command": "as.views.projects.services.deploy",
				"title": "Deploy service"
			},
			{
				"command": "as.views.projects.services.undeploy",
				"title": "Undeploy service"
			},
			{
				"command": "as.views.projects.services.expose",
				"title": "Expose service"
			},
			{
				"command": "as.views.projects.services.unexpose",
				"title": "Unexpose service"
			},
			{
				"command": "as.views.projects.openbrowser",
				"title": "View in browser"
			},
			{
				"command": "as.views.projects.details.projects",
				"title": "View project details"
			},
			{
				"command": "as.views.projects.details.services",
				"title": "View service details"
			},
			{
				"command": "as.views.projects.details.members",
				"title": "View member details"
			},
			{
				"command": "as.views.projects.details.credentials",
				"title": "View credential details"
			},
			{
				"command": "as.views.projects.details.invites",
				"title": "View invite details"
			},
			{
				"command": "as.views.projects.invites.inviteuser",
				"title": "Invite user"
			},
			{
				"command": "as.views.projects.invites.deleteinvite",
				"title": "Delete invitation for user"
			},
			{
				"command": "as.views.credentials.details.tokens",
				"title": "View token details"
			},
			{
				"command": "as.views.credentials.revoke",
				"title": "Revoke token"
			},
			{
				"command": "as.views.projects.new",
				"title": "Add new project",
				"category": "Akka Serverless"
			},
			{
				"command": "as.views.status.view",
				"title": "View Akka Serverless status page",
				"category": "Akka Serverless"
			},
			{
				"command": "as.views.tools.info",
				"title": "Get tool info"
			},
			{
				"command": "as.views.status.info",
				"title": "Get status info"
			},
			{
				"command": "as.commandpalette.invites.inviteuser",
				"title": "Invite user to project",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.invites.deleteinvite",
				"title": "Delete invitation for user from project",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.auth.login",
				"title": "Login to Akka Serverless",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.credentials.add",
				"title": "Add Docker credentials to an Akka Serverless project",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.credentials.delete",
				"title": "Delete Docker credentials from an Akka Serverless project",
				"category": "Akka Serverless"
			},
			{
				"command": "as.commandpalette.auth.logout",
				"title": "Logout from Akka Serverless",
				"category": "Akka Serverless"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "as.views.projects.new",
					"when": "view == as.views.projects",
					"group": "0"
				},
				{
					"command": "as.views.projects.refresh",
					"when": "view == as.views.projects",
					"group": "navigation"
				},
				{
					"command": "as.views.status.refresh",
					"when": "view == as.views.status",
					"group": "navigation"
				},
				{
					"command": "as.views.tools.refresh",
					"when": "view == as.views.tools",
					"group": "navigation"
				},
				{
					"command": "as.views.credentials.refresh",
					"when": "view == as.views.credentials",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "as.views.projects.credentials.add",
					"when": "view == as.views.projects && viewItem == Credentials",
					"group": "0"
				},
				{
					"command": "as.views.projects.credentials.delete",
					"when": "view == as.views.projects && viewItem == Credentials",
					"group": "0"
				},
				{
					"command": "as.views.projects.services.deploy",
					"when": "view == as.views.projects && viewItem == Services",
					"group": "0"
				},
				{
					"command": "as.views.projects.services.undeploy",
					"when": "view == as.views.projects && viewItem == Services",
					"group": "0"
				},
				{
					"command": "as.views.projects.services.expose",
					"when": "view == as.views.projects && viewItem == Services",
					"group": "0"
				},
				{
					"command": "as.views.projects.services.unexpose",
					"when": "view == as.views.projects && viewItem == Services",
					"group": "0"
				},
				{
					"command": "as.views.projects.details.projects",
					"when": "view == as.views.projects && viewItem == Projects",
					"group": "1"
				},
				{
					"command": "as.views.projects.details.services",
					"when": "view == as.views.projects && viewItem == Services",
					"group": "1"
				},
				{
					"command": "as.views.projects.details.credentials",
					"when": "view == as.views.projects && viewItem == Credentials",
					"group": "1"
				},
				{
					"command": "as.views.projects.details.members",
					"when": "view == as.views.projects && viewItem == Members",
					"group": "1"
				},
				{
					"command": "as.views.projects.details.invites",
					"when": "view == as.views.projects && viewItem == Invites",
					"group": "1"
				},
				{
					"command": "as.views.projects.invites.inviteuser",
					"when": "view == as.views.projects && viewItem == Invites",
					"group": "1"
				},
				{
					"command": "as.views.projects.invites.deleteinvite",
					"when": "view == as.views.projects && viewItem == Invites",
					"group": "1"
				},
				{
					"command": "as.views.tools.info",
					"when": "view == as.views.tools && viewItem == Tools"
				},
				{
					"command": "as.views.status.info",
					"when": "view == as.views.status"
				},
				{
					"command": "as.views.credentials.details.tokens",
					"when": "view == as.views.credentials && viewItem == Tokens",
					"group": "0"
				},
				{
					"command": "as.views.credentials.revoke",
					"when": "view == as.views.credentials && viewItem == Tokens",
					"group": "0"
				},
				{
					"command": "as.views.projects.openbrowser",
					"when": "view == as.views.projects",
					"group": "99"
				}
			],
			"commandPalette": [
				{
					"command": "as.views.projects.credentials.add",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.credentials.delete",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.services.deploy",
					"when": "view == as.views.projects",
					"group": "0"
				},
				{
					"command": "as.views.projects.services.undeploy",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.services.expose",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.services.unexpose",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.details.projects",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.details.services",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.details.credentials",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.details.members",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.details.invites",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.invites.inviteuser",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.projects.invites.deleteinvite",
					"when": "view == as.views.projects"
				},
				{
					"command": "as.views.tools.info",
					"when": "view == as.views.tools"
				},
				{
					"command": "as.views.status.info",
					"when": "view == as.views.status"
				},
				{
					"command": "as.views.credentials.details.tokens",
					"when": "view == as.views.credentials"
				},
				{
					"command": "as.views.credentials.revoke",
					"when": "view == as.views.credentials"
				},
				{
					"command": "as.views.projects.openbrowser",
					"when": "view == as.views.projects"
				}
			]
		}
	},
	"dependencies": {
		"@types/cli-table": "^0.3.0",
		"@types/shelljs": "^0.8.8",
		"axios": "^0.21.0",
		"cli-table3": "^0.6.0",
		"shelljs": "^0.8.4"
	}
}